<?xml version="1.0" encoding="utf-8" ?>
<ScrapWebDataConfig>
  <Scrap name="rgn" url="https://unstats.un.org/unsd/methodology/m49/" xpath="//table[@id='GeoGroupsENG']/tr[position() > 1]" type="table">
    <Column name="m49" xpath="td[1]" ispk="1"></Column>
    <Column name="nm" xpath="./data-tt-id"></Column>
    <Column name="m49p" xpath="./data-tt-parent-id"></Column>
    <Column name="iso31661a3" xpath="td[3]"></Column>
  </Scrap>
  <Scrap name="rgn" url="https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2" xpath="//div[@class='mw-parser-output']/table[3]" type="table">
    <Column name="iso31661a2" xpath="td[1]"></Column>
    <Column name="nm" ispk="1" xpath="td[2]"></Column>
    <Column name="tld" xpath="td[4]"></Column>
    <Column name="iso31661a2yrentr" xpath="td[3]"></Column>
  </Scrap>
  <Scrap name="rgn" url="https://evs.nci.nih.gov/ftp1/FDA/GENC/NCIt-GENC_Terminology.txt" type="csv">
    <Column name="nm" index="1" ispk="1"></Column>
    <Column name="genc_2a" index="3"></Column>
    <Column name="genc_3a" index="4"></Column>
    <Column name="genc_no" index="5"></Column>
  </Scrap>
  <Scrap name="rgn" url="http://www.geonames.org/countries/" type="table" xpath="//table[@id='countries']">
    <Column name="iso31661a2" xpath="td[1]" ispk="1"></Column>
    <Column name="iso3166_no" xpath="td[3]"></Column>
    <Column name="fips" xpath="td[4]"></Column>
    <Column name="areakm2" xpath="td[7]"></Column>
  </Scrap>
  <Scrap name="lang" url="http://www.mathguide.de/info/tools/languagecode.html" type="table" xpath="//body/p[4]/table//table//tr[position() > 1]" tableinsfresh="1">
    <Column name="iso6391" xpath="td[1]"></Column>
    <Column name="nm" xpath="td[2]"></Column>
  </Scrap>
  <Scrap name="cntrylang" url="http://www.geonames.org/countries/" type="tablehref" xpath="//table[@id='countries']//tr[position()>1]//td[5]/@href">
    <Column name="iso31661a2" xpath="//body/div[2]/table/tr[2]/td[2]">
      <Manipulate>
        <Split data="," index="2"></Split>
      </Manipulate>
    </Column>
    <Column name="iso6391" xpath="td[3]"></Column>
  </Scrap>
  <Scrap name="cntrylang" url="http://www.xe.com/iso4217.php" type="table" xpath="//table[@id='currencyTable']//tr[position()>1]">
    <Column name="iso4217" xpath="td[1]"></Column>
    <Column name="nm" xpath="td[2]"></Column>
  </Scrap>
  <Scrap name="crncy" url="http://www.xe.com/iso4217.php" type="tablehref" xpath="//table[@id='currencyTable']//tr[position()>1]/a/@href">
    <Column name="symbl" xpath="//div[@id='contentL']/div[3]/div[1]/p[2]/text()[2]"></Column>
    <Column name="mnrunm" xpath="//div[@id='contentL']/div[3]/div[1]/p[3]/text()[2]">
      <Manipulate>
        <Split data="=" index="1"></Split>
      </Manipulate>
    </Column>
    <Column name="cntrlbnk" xpath="//div[@id='contentL']/div[3]/div[2]/p[4]/text()[2]"></Column>
  </Scrap>
  <Scrap name="crncycntry" url="http://www.xe.com/iso4217.php" type="tablehref" xpath="//table[@id='currencyTable']//tr[position()>1]/a/@href">
    <Column name="iso4217" xpath="//div[@id='contentL']/div[3]/div/div">
      <Manipulate>
        <Split data=" " index="0"></Split>
      </Manipulate>
    </Column>
    <Column name="m49" xpath="//div[@id='contentL']/div[3]/div[2]/p[6]/text()[2]">
      <Manipulate>
        <XtarctToCol></XtarctToCol>
      </Manipulate>
    </Column>
  </Scrap>
</ScrapWebDataConfig>